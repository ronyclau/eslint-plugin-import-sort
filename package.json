{
  "name": "eslint-plugin-import-sort",
  "version": "0.1.0",
  "description": "Runs import-sort as an eslint rule",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "import-sort"
  ],
  "author": "Ron Lau",
  "files": [
    "eslint-plugin-import-sort.js"
  ],
  "main": "eslint-plugin-import-sort.js",
  "scripts": {
    "lint": "eslint .",
    "test": "npm run lint",
    "format": "yarn run prettier '**/*.{js,json,md,yml}' --write && yarn run lint --fix",
    "generate-release": "node-release-script"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ronyclau/eslint-plugin-import-sort.git"
  },
  "bugs": {
    "url": "https://github.com/prettier/eslint-plugin-import-sort/issues"
  },
  "homepage": "https://github.com/prettier/eslint-plugin-import-sort",
  "dependencies": {
    "prettier-linter-helpers": "^1.0.0"
  },
  "peerDependencies": {
    "eslint": ">= 5.0.0",
    "import-sort": ">= 6.0.0"
  },
  "devDependencies": {
    "@not-an-aardvark/node-release-script": "^0.1.0",
    "eslint": "^6.0.0",
    "eslint-config-not-an-aardvark": "^2.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-eslint-plugin": "^2.0.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-self": "^1.1.0",
    "prettier": "^1.15.3",
    "vue-eslint-parser": "^6.0.0"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "license": "MIT"
}
